    "babel-loader": "5.1.4",
    "babel-polyfill": "6.6.1",
    "babel-runtime": "5.5.8",
    "bluebird": "3.3.3",
    "bootstrap-sass": "3.3.6",
    "compression": "1.5.0",
    "cors": "2.7.1",
    "css-loader": "0.14.5",
    "dom-scroll-into-view": "1.1.0",
    "expect": "1.6.0",
    "exports-loader": "0.6.3",
    "express": "4.12.4",
    "file-loader": "0.8.5",
    "fuse.js": "github:krisk/Fuse#v2.0.1",
    "html-webpack-plugin": "1.5.2",
